<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background"
    tools:context=".TradingActivity">

    <include
        layout="@layout/trading_layout"
        android:id="@+id/trading_layout" />

    <com.macduy.games.fstock.view.EffectsView
        android:id="@+id/effects_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        layout="@layout/game_summary"
        android:id="@+id/game_summary" />

    <View
        android:id="@+id/test"
        android:alpha="0.5"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_gravity="center"
        android:background="#ff23583f"
        />

</FrameLayout>